{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordStrength": {
    "MinLength": 10,
    "RequireUppercase": true,
    "RequireLowercase": true,
    "RequireDigit": true,
    "RequireSpecialChar": true,
    "SpecialChars": "!@#$%^&*()-_=+[]{}|;:',.<>/?`~"
  }
}
